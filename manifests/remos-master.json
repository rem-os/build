{
  "os-name": "RemOS",
  "base-packages": {
    "world-flags": {
      "default": [
        "MALLOC_PRODUCTION=YES",
        "WITHOUT_ASSERT_DEBUG=ON"
      ]
    },
    "kernel-flags": {
      "default": [
        "KERNCONF=GENERIC-NODEBUG"
      ]
    },
    "branch": "remos-master",
    "repo": "https://github.com/rem-os/remos"
  },
  "iso": {
    "auto-install-packages": {
      "default": [
        "security/ca_root_nss",
        "sysutils/dmidecode",
        "sysutils/ipmitool",
        "textproc/jq",
        "security/sudo",
        "sysutils/sysup",
        "sysutils/tmux"
      ]
    },
    "auto-install-script": "",
    "dist-packages": {
      "default": [
      ]
    },
    "dist-packages-glob": {
      "default": [
        "os-minimal-kernel",
        "os-minimal-userland",
        "os-nozfs-kernel",
        "os-nozfs-userland"
      ]
    },
    "file-name": "remos-master-x64-%%DATE%%-%%GITHASH%%-%%DATE%%",
    "install-script": "",
    "iso-packages": {
      "default": [
        "sysutils/pc-installdialog",
        "sysutils/pc-sysinstall",
        "textproc/jq"
      ]
    },
    "os-flavors": {
      "generic": {
        "description": "Default TrueOS world / kernel"
      },
      "minimal": {
        "description": "Minimal world with less optional features."
      },
      "nozfs": {
        "description": "Base system without built-in ZFS"
      },
      "openzfs": {
        "description": "Base system with OpenZFS from ports",
        "flavor": "nozfs",
        "packages": "openzfs"
      }
    },
    "overlay": {},
    "prune": {
      "default": [
        "METALOG",
        "/usr/local/share/examples",
        "/usr/local/include",
        "/usr/bin/cc*",
        "/usr/bin/clang*",
        "/usr/bin/cpp",
        "/usr/bin/cpp",
        "/usr/bin/c++",
        "/usr/bin/lldb",
        "/usr/bin/ld.lld",
        "/usr/bin/llvm*",
        "/usr/bin/objdump*",
        "/usr/bin/svn*",
        "/usr/lib/clang",
        "/usr/share/i18n",
        "/usr/include",
        "/usr/lib32"
      ]
    }
  },
  "pkg-repo": { 
    "url": "http://labstor02.lab.ixsystems.com:9000/ixlab/pkg/remos-master/latest",
    "signature_type": "none",
    "rclone_type": "s3",
    "rclone_provider": "minio",
    "rclone_transfers": "128",
    "rclone_auth":	"env"
  },
  "pkg-repo-name": "rem-os",
  "pkg-train-name": "remos",
